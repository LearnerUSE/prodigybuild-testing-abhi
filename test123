```
#test123.py

import math

# Function to check if number is prime
def is_prime(n):
    if n <= 1:
        return False
    if n == 2:
        return True
    if n % 2 == 0:
        return False
    sqr = int(math.sqrt(n)) + 1
    for divisor in range(3, sqr, 2):
        if n % divisor == 0:
            return False
    return True

# Function to add two prime numbers
def add_prime_nums(num1, num2):
    if is_prime(num1) and is_prime(num2):
        return num1 + num2
    else:
        return "One or both numbers are not prime"

# Test the function
print(add_prime_nums(2, 3))

